      return rest.length ? doMerge.call.apply(doMerge, [null, true, false, a, b, c, d, e, f].concat(rest)) : doMerge(true, false, a, b, c, d, e, f);
    }
    var timm = {
      clone,
      addLast: addLast2,
      addFirst,
      removeLast,
      removeFirst,
      insert,
      removeAt,
      replaceAt,
      getIn: getIn2,
      // eslint-disable-next-line object-shorthand
      set: set3,
      // so that flow doesn't complain
      setIn: setIn4,
      update,
      updateIn,
