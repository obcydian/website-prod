      }
    };
    transformDefaults = {
      [TRANSFORM_MOVE2]: Object.freeze({
        xValue: 0,
        yValue: 0,
        zValue: 0
      }),
      [TRANSFORM_SCALE2]: Object.freeze({
        xValue: 1,
        yValue: 1,
        zValue: 1
      }),
