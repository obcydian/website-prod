function getInstanceId() {
  return "i" + instanceCount++;
}
function getElementId(ixElements3, ref) {
  for (const key in ixElements3) {
    const ixEl = ixElements3[key];
    if (ixEl && ixEl.ref === ref) {
      return ixEl.id;
    }
  }
  return "e" + elementCount++;
