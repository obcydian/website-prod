  "../../app/packages/systems/ix2/engine/reducers/IX2SessionReducer.ts"() {
    "use strict";
    init_shared_constants();
    import_timm2 = __toESM(require_timm());
    ({
      IX2_SESSION_INITIALIZED: IX2_SESSION_INITIALIZED2,
