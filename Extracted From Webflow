      return result;
    };
    reduceFontVariations = (result, fontVariation) => {
      if (fontVariation) {
        result[fontVariation.type] = fontVariation.value || 0;
      }
      return result;
    };
    getItemConfigByKey = (actionTypeId, key, config) => {
      if (isPluginType(actionTypeId)) {
